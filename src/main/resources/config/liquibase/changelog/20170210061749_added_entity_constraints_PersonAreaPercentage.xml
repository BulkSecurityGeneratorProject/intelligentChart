<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity PersonAreaPercentage.
    -->
    <changeSet id="20170210061749-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="person_area_percentage"
                                 constraintName="fk_person_area_percentage_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

        <addForeignKeyConstraint baseColumnNames="media_type_id"
                                 baseTableName="person_area_percentage"
                                 constraintName="fk_person_area_percentage_media_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="media_type"/>

        <addForeignKeyConstraint baseColumnNames="area_type_id"
                                 baseTableName="person_area_percentage"
                                 constraintName="fk_person_area_percentage_area_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="area_type"/>

    </changeSet>
</databaseChangeLog>
