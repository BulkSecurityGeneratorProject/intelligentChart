<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity MovieParticipant.
    -->
    <changeSet id="20170421022149-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="movie_id"
                                 baseTableName="movie_participant"
                                 constraintName="fk_movie_participant_movie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="movie"/>

        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="movie_participant"
                                 constraintName="fk_movie_participant_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

        <addForeignKeyConstraint baseColumnNames="job_id"
                                 baseTableName="movie_participant"
                                 constraintName="fk_movie_participant_job_id"
                                 referencedColumnNames="id"
                                 referencedTableName="job"/>

    </changeSet>
</databaseChangeLog>
